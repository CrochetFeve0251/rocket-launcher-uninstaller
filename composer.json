{
    "name": "wp-launchpad/uninstaller",
    "description": "Uninstaller from Launchpad framework",
    "keywords": [
        "wordpress"
    ],
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "CrochetFeve0251"
        }
    ],
    "autoload": {
        "psr-4": {
            "LaunchpadUninstaller\\": "inc/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaunchpadUninstaller\\Tests\\": "tests/"
        }
    },
    "require": {
        "wp-launchpad/core": "^3.1"
    },
    "require-dev": {
        "wp-media/phpunit": "^3.0"
    },
    "scripts": {
        "test-integration": "\"vendor/bin/phpunit\" --testsuite integration --colors=always --configuration tests/Integration/phpunit.xml.dist --exclude-group AdminOnly",
        "run-tests": [
            "@test-integration"
        ]
    }
}
